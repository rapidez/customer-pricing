{
    "name": "rapidez/customer-pricing",
    "description": "Support for the justbetter/magento2-customer-pricing magento extension",
    "keywords": [
        "rapidez",
        "customer-pricing"
    ],
    "homepage": "https://github.com/rapidez/customer-pricing",
    "license": "GPL-3.0",
    "authors": [
        {
            "name": "Jade Geels",
            "email": "jade@justbetter.nl",
            "role": "Developer"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "php": "^8.1|^8.2",
        "rapidez/core": "^1.0|^2.0"
    },
    "autoload": {
        "psr-4": {
            "Rapidez\\CustomerPricing\\": "src"
        }
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Rapidez\\CustomerPricing\\CustomerPricingServiceProvider"
            ]
        }
    }
}
